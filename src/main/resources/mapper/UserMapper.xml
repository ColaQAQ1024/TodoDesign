<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.todoDesign.mapper.UserMapper">

    <select id="getUserByUsername" resultType="com.todoDesign.entity.User">
        select * from demos.todo_user
        where username = #{userName}
    </select>
</mapper>
